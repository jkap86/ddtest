er\">\n        {props.names.map(name =>\n            <span key={name} id={name} className={selection[props.name] === name ? 'selected' : 'option'}>\n                <button className='clickable' name={name} onClick={slide}>{name}</button>\n            </span>\n        )}\n    </div>\n}\n\nexport default SliderToggle;",{"ruleId":"105","severity":1,"message":"106","line":1,"column":20,"nodeType":"107","messageId":"108","endLine":1,"endColumn":29},{"ruleId":"109","severity":1,"message":"110","line":27,"column":22,"nodeType":"111","messageId":"112","endLine":27,"endColumn":24},{"ruleId":"105","severity":1,"message":"113","line":51,"column":23,"nodeType":"107","messageId":"108","endLine":51,"endColumn":24},{"ruleId":"109","severity":1,"message":"110","line":51,"column":59,"nodeType":"111","messageId":"112","endLine":51,"endColumn":61},{"ruleId":"114","severity":1,"message":"115","line":132,"column":81,"nodeType":"116","messageId":"117","endLine":132,"endColumn":109},{"ruleId":"114","severity":1,"message":"115","line":133,"column":81,"nodeType":"116","messageId":"117","endLine":133,"endColumn":109},{"ruleId":"109","severity":1,"message":"110","line":62,"column":31,"nodeType":"111","messageId":"112","endLine":62,"endColumn":33},{"ruleId":"118","severity":1,"message":"119","line":77,"column":8,"nodeType":"120","endLine":77,"endColumn":26,"suggestions":"121"},{"ruleId":"109","severity":1,"message":"110","line":81,"column":54,"nodeType":"111","messageId":"112","endLine":81,"endColumn":56},{"ruleId":"109","severity":1,"message":"110","line":89,"column":54,"nodeType":"111","messageId":"112","endLine":89,"endColumn":56},{"ruleId":"109","severity":1,"message":"110","line":107,"column":33,"nodeType":"111","messageId":"112","endLine":107,"endColumn":35},{"ruleId":"122","severity":1,"message":"123","line":121,"column":35,"nodeType":"124","messageId":"125","endLine":121,"endColumn":46},{"ruleId":"109","severity":1,"message":"110","line":12,"column":61,"nodeType":"111","messageId":"11